include "uuid.fbs";
include "crypto.fbs";

namespace lumos.registry;

enum EntryType:ubyte {
    UPSERT,
    DELETE
}

table Entry {
    uuid: UUID (required);
    parent: UUID (required);

    type: EntryType;

    key: string (required);
    value: [ubyte];

    signature: crypto.Signature;
}

file_identifier "REGE";
root_type Entry;
